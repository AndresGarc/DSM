<?xml version="1.0" encoding="utf-8" ?>
		<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
		                   assembly="SuperPetGenNHibernate"
		                   namespace="SuperPetGenNHibernate.EN.BaseDatos">
		    
		    
			
				<class name="FacturaEN" table="Factura"> 
				  	
						<id name="Id" column="idFactura" type="integer"> 
							<generator class="hilo"/>
						</id>
			
					
					<property name="PrecioTotal" type="float"> 
						<column name="precioTotal"  not-null="true"/>
					</property>
					<property name="IVA" type="float"> 
						<column name="IVA"  not-null="true"/>
					</property>
			
			
			
			
		  			
					
							<many-to-one name="Pedido" class="SuperPetGenNHibernate.EN.BaseDatos.PedidoEN" >
								<column name="FK_idPedido_idPedido"/> 
        					</many-to-one>
					 
						
			
					
					
					
					
		
		 </class>	
		  
		  <query name="FacturaENReadAllHQL">from FacturaEN</query>
		</hibernate-mapping>
	